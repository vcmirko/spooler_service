{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "pretty": {
      "format": "%(asctime)s [%(levelname)s] %(name)s - %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "pretty",
      "level": "INFO"
    },
    "file": {
      "class": "logging.FileHandler",
      "filename": "spooler_service.log",
      "formatter": "standard",
      "level": "INFO"
    },
    "null": {
      "class": "logging.NullHandler"
    }
  },
  "loggers": {
    "": {
      "handlers": [
        "console",
        "file"
      ],
      "level": "INFO"
    },
    "flow_processor.api": {
      "handlers": [
        "console",
        "file"
      ],
      "level": "INFO",
      "propagate": false
    },
